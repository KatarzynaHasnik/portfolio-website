function addToOutput(text) {
  var output = document.getElementById("output");
  output.innerHTML = output.innerHTML += "<br>" + text;
}

document.getElementById("start").onclick = function() {
  for(var i = 0; i < 100; i++) {
    if(checkNumber(i)) {
      addToOutput(i);
    }
  }
}
